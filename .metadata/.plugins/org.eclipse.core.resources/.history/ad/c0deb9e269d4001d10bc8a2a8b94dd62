package ch08;

public class CharaterTest {

	public static void main(String[] args) {
		char chA = 'A';
		System.out.println(chA);
		System.out.println((int)chA);

		char chB = 66;
		System.out.println(chB);
		System.out.println((char)chB);
		
		int chC = 67;
		System.out.println(chC);
		System.out.println((char)chC);
	}

}
